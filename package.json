{
  "name": "apitally",
  "version": "1.0.0",
  "description": "Apitally client library for Node.js",
  "author": "Apitally <hello@apitally.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/apitally/nodejs-client.git"
  },
  "bugs": {
    "url": "https://github.com/apitally/nodejs-client/issues"
  },
  "homepage": "https://apitally.io",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "check": "tsc --noEmit && eslint src tests && prettier --check .",
    "test": "jest"
  },
  "files": [
    "dist/**/*",
    "!dist/tests/",
    "README.md",
    "LICENSE"
  ],
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": "off"
    },
    "env": {
      "node": true
    },
    "root": true
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coverageReporters": [
      "text",
      "lcovonly"
    ],
    "collectCoverageFrom": [
      "src/**/*"
    ]
  },
  "dependencies": {
    "axios": "^1",
    "axios-retry": "^3",
    "winston": "^3"
  },
  "peerDependencies": {
    "@nestjs/common": "^8 || ^9 || ^10",
    "@nestjs/core": "^8 || ^9 || ^10",
    "@nestjs/platform-express": "^8 || ^9 || ^10",
    "express": "^4",
    "fastify": "^3 || ^4",
    "fastify-plugin": "^3 || ^4",
    "koa": "^2"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    },
    "@nestjs/core": {
      "optional": true
    },
    "@nestjs/platform-express": {
      "optional": true
    },
    "express": {
      "optional": true
    },
    "fastify": {
      "optional": true
    },
    "fastify-plugin": {
      "optional": true
    },
    "koa": {
      "optional": true
    }
  },
  "devDependencies": {
    "@koa/router": "^12.0.1",
    "@nestjs/common": "^10.2.9",
    "@nestjs/core": "^10.2.9",
    "@nestjs/platform-express": "^10.2.9",
    "@nestjs/testing": "^10.2.9",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/koa": "^2.13.11",
    "@types/koa__router": "^12.0.4",
    "@types/koa-route": "^3.2.8",
    "@types/node": "^20.9.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "celebrate": "^15.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "eslint": "^8.54.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "fastify": "^4.24.3",
    "fastify-plugin": "^4.5.1",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "koa": "^2.14.2",
    "koa-route": "^3.2.0",
    "nock": "^13.3.8",
    "npm": "^10.2.4",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
